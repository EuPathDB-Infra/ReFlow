#!/usr/bin/perl

use lib "$ENV{GUS_HOME}/lib/perl";
use FgpUtil::Runtime::CommandHelper;

my $GUS_HOME = $ENV{GUS_HOME};
my $CLASSPATH = &FgpUtil::Runtime::CommandHelper::getJavaClasspath($GUS_HOME);
my $sysProps = &FgpUtil::Runtime::CommandHelper::getSystemProps($GUS_HOME, 'workflow');
my $args = &FgpUtil::Runtime::CommandHelper::getJavaArgs(@ARGV);

my $cmd = "java $sysProps -classpath $CLASSPATH org.gusdb.workflow.Workflow $args";

system($cmd);

exit($? >> 8);
