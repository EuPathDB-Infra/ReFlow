<workflowGraph name="sub">

  <param name="prefix"/>

  <step name="fourthStep"
        stepClass="ReFlow::TestFlow::TestStep">
      <paramValue name="msg">$$prefix$$/purple</paramValue>
  </step>

  <step name="fifthStep"
        stepClass="ReFlow::TestFlow::TestStep">
      <paramValue name="msg">black</paramValue>
    <depends name="fourthStep"/>
  </step>

</workflowGraph>
